{
    "name": "Tabs Display",
    "description" : "Base Level Extension",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "tabs.html",
      "default_icon": "scaledWindow.png"
    },
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+F",
          "mac": "MacCtrl+Shift+F"
        },
        "description": "tabs List"
      }
    },
    "permissions": [
        "tabs", "bookmarks"
    ]
  
  }
